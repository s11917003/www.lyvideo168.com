{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "chrisyue/php-m3u8": "^2.3",
        "gumlet/php-image-resize": "1.9.*",
        "ideneal/openload": "~1.1",
        "intervention/image": "^2.6",
        "ipunkt/laravel-analytics": "^1.3",
        "jaeger/querylist": "^3.2",
        "laravel/framework": "^5.5.48",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "maatwebsite/excel": "~2.1.0",
        "php-ffmpeg/php-ffmpeg": "^0.11.1",
        "predis/predis": "^1.1",
        "rap2hpoutre/fast-excel": "^3.0",
        "srmklive/paypal": "~1.0",
        "watson/rememberable": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0.8",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
